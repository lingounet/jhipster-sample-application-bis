<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Identity.
    -->
    <changeSet id="20220524125425-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="psat_id"
                                 baseTableName="identity"
                                 constraintName="fk_identity__psat_id"
                                 referencedColumnNames="id"
                                 referencedTableName="psat"/>

        <addForeignKeyConstraint baseColumnNames="availability_id"
                                 baseTableName="identity"
                                 constraintName="fk_identity__availability_id"
                                 referencedColumnNames="id"
                                 referencedTableName="availability"/>

        <addForeignKeyConstraint baseColumnNames="application_type_id"
                                 baseTableName="identity"
                                 constraintName="fk_identity__application_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application_type"/>
    </changeSet>
</databaseChangeLog>
