<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity PersonalData.
    -->
    <changeSet id="20220524125427-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="personal_data_type_id"
                                 baseTableName="personal_data"
                                 constraintName="fk_personal_data__personal_data_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personal_data_type"/>

        <addForeignKeyConstraint baseColumnNames="identity_id"
                                 baseTableName="personal_data"
                                 constraintName="fk_personal_data__identity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="identity"/>
    </changeSet>
</databaseChangeLog>
